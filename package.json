{
  "name": "omnia-bundler",
  "description": "An in place code transpiler to be used in multi page sites",
  "version": "1.0.31",
  "author": "bryanshrugged",
  "bin": {
    "omnia-bundler": "./bin/run"
  },
  "bugs": "https://github.com/node-cli/omnia-bundler/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "ora": "^3.0.0",
    "preact": "^8.3.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "tslib": "^1",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/ora": "^1.3.4",
    "@types/webpack": "^4.4.11",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^12",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "babel-preset-flow": "^6.23.0",
    "css-loader": "^1.0.0",
    "preact": "^8.3.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "style-loader": "^0.23.0",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "ie 11"
  ],
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/node-cli/omnia-bundler",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "omnia-bundler",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "node-cli/omnia-bundler",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
